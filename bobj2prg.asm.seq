.PAGE 'OBJ2PRG".OPT LIST;**********************************=$0801        ;START OF BASIC;*********************************; SET BSTUB TO ONE TO ALLOW FOR; INSERTION OF BASIC STUB CODE;*********************************BSTUB = 1;*********************************;;         OBJ2PRG64;      OBJ2PRG.C64 V072882;CONVERT OBJECT FILE FROM ASSEMBLER64;        TO PROGRAM FILE; (C) 1982 BY COMMODORE MACHINES;;  WRITTEN BY DENTON MARLOWE;*********************************;; BASIC FORMAT PROGRAM;; 10 SYS(2063);;*********************************.IFN BSTUB < .WORD BASIC    ;LINE LINK POINTER .BYTE $0A,$00  ;LINE NUMBER (10) .BYTE 158      ;SYS COMMAND TOKEN .BYTE '(2063)' ;CALL TO $080F .BYTE $00      ;END OF LINEBASIC .BYTE $00,$00  ;END OF BASIC>;*********************************;*=$080F        ;LOADER64 ORGIN;*********************************; SET FLAG TO ALLOW BASIC; AND KERNAL (ERROR) MESSAGES; $80 FOR BASIC; $40 FOR KERNAL;*********************************SETMSG=$FF90 LDA #$40     ;ALLOW KERNAL MESSAGES JSR SETMSG;*********************************; MAIN BODY OF CODE;*********************************.FIL OBJ2PRG.ASM;*********************************.END